defn last :: [a] -> a {
  "Docstring that can
be as many lines as you want."
    [] -> error badarg
    [x] -> x
    [_|xs] -> last xs
}

defn tail :: [a] -> [a] {
  "Return the tail
of a collection."
    [] -> error badarg
    [_|xs] -> xs
}

defn head :: [a] -> a {
  "Return the first element
of a collection."
    [] -> error badarg
    [x|_] -> x
}

